SELECT
    Event_ID,
    Calendared,
    CASE
        WHEN Calendared IS NULL OR Calendared = '' THEN
            CASE
                WHEN EXISTS (
                    SELECT 1 FROM events e WHERE e.Event_ID = events.Event_ID AND e.Calendared > '0'
                ) THEN 'Yes'
                ELSE 'No'
            END
        ELSE Calendared
    END AS est_calendared_check
FROM events
WHERE Event_ID = 'specific_event_id';
