CASE
    WHEN draft_to_prudential_date IS NOT NULL THEN
        CASE
            WHEN exam_analysis_finalized_date < '1000-01-01' OR exam_analysis_finalized_date > '9999-12-31' THEN NULL
            ELSE sv3_rpt.workday_optimized(exam_analysis_finalized_date::date, 45)
        END
    WHEN draft_to_prudential_date IS NULL AND exam_mail_date IS NOT NULL THEN
        CASE
            WHEN exam_analysis_finalized_date < '1000-01-01' OR exam_analysis_finalized_date > '9999-12-31' THEN NULL
            ELSE sv3_rpt.workday_optimized(exam_analysis_finalized_date::date, 45)
        END
    ELSE NULL
END AS "GPRA Due Date",
