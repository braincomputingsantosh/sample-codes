# Define the columns and their data types
columns = 'DHCST TEXT, STUSAB TEXT, SUMLEVEL INTEGER'

# Function to convert the column definition to a SQL CREATE TABLE script
def create_table_script(table_name, columns):
    # Split the columns string into individual column definitions
    column_definitions = columns.split(', ')
    # Start the CREATE TABLE script
    script = f"CREATE TABLE {table_name} (\n"
    # Add each column definition to the script
    for column in column_definitions:
        script += f"    {column},\n"
    # Remove the last comma and add closing parenthesis
    script = script.rstrip(',\n') + '\n);'
    return script

# Example usage
table_name = "DHC"
sql_script = create_table_script(table_name, columns)
print(sql_script)
